    "sane": "^4.0.2",
    "sinon": "^7.1.0",
    "start-server-and-test": "^1.7.5",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^2.0.0",
